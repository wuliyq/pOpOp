{
  "manifest_version": 3,
  "name": "Chaos vs Order Window Manager",
  "version": "1.0",
  "permissions": [
    "windows",
    "tabs",
    "storage",
    "system.display"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "pOpOp",
    "default_popup": "popop.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
